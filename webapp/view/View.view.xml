<mvc:View 
	controllerName="SurveyApp.controller.View" 
	xmlns:html="http://www.w3.org/1999/xhtml" 
	xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" 
	xmlns="sap.m">
	<App>
		<pages>
			<Page title="{i18n>title}">
				<content>
					<Button
						class="sapUiSmallMarginBottom"
						text="Open Survey"
						press="onPress">
					</Button>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>